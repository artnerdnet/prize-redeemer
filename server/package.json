{
  "name": "server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "client": "npm run start --prefix client",
    "start:dev": "nodemon --inspect ./src/server.js",
    "test": "jest --config ./jest.config.json",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "test:watch": "yarn test . --watch",
    "test:docker": "yarn docker:up && yarn prisma migrate deploy && jest -i --watch"
  },
  "prisma": {
    "seed": "node ./prisma/seed.js --compiler-options {\"module\":\"CommonJS\"}"
  },
  "imports": {
    "#config/*": "./config/*",
    "#controllers/*": "./src/controllers/*",
    "#helpers/*": "./src/helpers/*.js",
    "#lib/*": "./src/lib/*",
    "#routes/*": "./src/routes/index.js",
    "#services/*": "./src/services/*",
    "#testUtils/*": "./testUtils/*"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "3.12.0",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-session": "^1.17.2",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "morgan-json": "^1.1.0",
    "mysql2": "^2.3.3",
    "uuid": "^8.3.2",
    "winston": "^3.6.0",
    "winston-logsene": "^2.1.1"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.18.0",
    "@jest-mock/express": "^1.4.5",
    "babel-jest": "^28.1.0",
    "eslint": "^7.32.0",
    "jest": "^28.1.0",
    "jest-mock-extended": "^2.0.6",
    "nodemon": "^2.0.14",
    "prisma": "3.12.0",
    "prisma-mock": "^0.0.27"
  }
}
